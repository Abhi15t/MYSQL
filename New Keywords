//Left & 1 indicates first letter in string from left & for ending wise versa.
//Not in indicate not from the specified set.
SELECT DISTINCT CITY FROM STATION
WHERE LEFT(CITY,1) NOT IN ('a','e','i','o','u') AND RIGHT(CITY,1) NOT IN ('a','e','i','o','u');

SELECT NAME from STUDENTS
WHERE Marks>75
ORDER BY RIGHT (NAME, 3), ID ASC, MARKS DESC;
